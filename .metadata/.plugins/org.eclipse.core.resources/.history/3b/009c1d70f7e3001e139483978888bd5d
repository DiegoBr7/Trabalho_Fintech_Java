package Usuario;

public class TesteUsuario {

	public static void main(String[] args) {
		
	  Usuario usuario = new Usuario() ;
	  
	  usuario.setNomeCompleto("");
	  usuario.setNrCPF("");
	  usuario.setCdUsuario("");
      usuario.setDsSexo('M');
      usuario.setDsProfissao("");
      usuario.setDsEmail("");
       
		
	  
	  System.out.println("Nome do usuario: " + usuario.getNomeCompleto( );
	  System.out.println("CPF do usuario: " + usuario.getNrCPF() );
	  System.out.println("cdUsuario do usuario: " + usuario.getCdUsuario() );
	  System.out.println("Sexo do usuario: " + usuario.getDsSexo() );
	  System.out.println("Profissão do usuario: " + usuario.getDsProfissao() );
	  System.out.println("Email do usuario: " + usuario.getDsEmail() );
	  
	  
      // Exemplo de cadastro de endereço
      usuario.cadastrarEndereco("Rua Exemplo", 123, "Bairro Exemplo", "Cidade Exemplo", "Estado Exemplo", "12345-678");
      
      // Exemplo de cadastro de telefone
      usuario.cadastrarTelefone(123456789);
      
      // Exemplo de cadastro de carteira
      usuario.cadastrarCarteira(100.0);
      
      // Exemplo de cadastro de cartão de crédito
      usuario.cadastrarCartaoDeCredito(987654321);
      
      // Exemplo de cadastro de banco
      usuario.cadastrarBanco("Banco XYZ");
	  


	}

}
